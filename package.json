{
  "name": "base2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "js:dev": "webpack --watch --devtool inline-source-map --debug",
    "js:production": "webpack --watch -p",
    "style:dev": "postcss -w --map.inline -c postcss.js -o dist/style/style.css app/style/style.css",
    "style:production": "postcss -w -c postcss.js cssnano -o dist/style/style.css app/style/style.css",
    "pug": "pug app/pug/files -w -p --name-after-file -P -O ./config.json --out ./dist",
    "browsersync": "browser-sync start --server 'dist' --files 'dist' --reloadDebounce 250 --loglevel 'debug'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "npm-run-all -p js:dev style:dev pug browsersync ",
    "p": "npm-run-all js:production style:production pug browsersync"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "browser-sync": "^2.18.2",
    "chokidar-cli": "^1.2.0",
    "cross-env": "^3.1.3",
    "css-mqpacker": "^5.0.1",
    "cssnano": "^3.8.1",
    "mkdirp": "^0.5.1",
    "node-notifier": "^4.6.1",
    "npm-run-all": "^3.1.1",
    "postcss": "^5.2.6",
    "postcss-apply": "^0.4.0",
    "postcss-cli": "^2.6.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-flexbugs-fixes": "^2.0.0",
    "postcss-font-magician": "^1.5.0",
    "postcss-for": "^2.1.1",
    "postcss-import": "^8.2.0",
    "postcss-nesting": "^2.3.1",
    "postcss-sorting": "^1.7.0",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.4",
    "webpack": "2.1.0-beta.27"
  },
  "dependencies": {
    "normalize.css": "^5.0.0"
  }
}
